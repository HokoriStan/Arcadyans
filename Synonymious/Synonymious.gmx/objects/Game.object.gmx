if (flashHintCount &gt; 0)
{
    flashHintCount -= 6/room_speed;
    if (flashHintCount &lt; 0)
    {
        flashHintCount = 0;
    }
}
if (flashRemoveLetterCount &gt; 0)
{
    flashRemoveLetterCount -= 6/room_speed;
    if (flashRemoveLetterCount &lt; 0)
    {
        flashRemoveLetterCount = 0;
    }
}

if (keyboard_check_pressed(vk_space))
{
    if (room == rm_game &amp;&amp; Game.level &lt; array_length_1d(Game.keyArray)-1)
    {
        Swipe(1);
        Game.level ++;
        Game.levelSolved = false;
        with (obj_inputBox)
        {
            instance_destroy();
        }
        with (obj_letterBox)
        {
            instance_destroy();
        }
        CreateLevel(Game.level);
    }
}
if ( (keyboard_check_pressed(vk_backspace) || keyboard_check_pressed(vk_escape)) &amp;&amp; !instance_exists(obj_message))
{
    switch (room)
    {
        case rm_game:
            //if (show_question("Are you sure you would like to go back to the main menu?"))
            {
                room_goto(rm_levelSelect);
            }
        break;
        case rm_menu:
            var text;
            switch (Game.language)
            {
                case "English": text = "Are you sure you# would like to quit?";  break;
                case "Dutch":   text = "Weet u zeker dat# u wilt stoppen?";      break;
                case "French":  text = "Etes-vous sûr# vous voulez quitter?";    break;
                case "Swedish": text = "Är du säker på att# du vill avsluta?";   break;
            }
            ShowMessage(text,true,MessageActionQuit);
        break;
        case rm_levelSelect:
            room_goto(rm_menu);
        break;
        case rm_languageSelection:
            room_goto(rm_menu);
        break;
        case rm_credits:
            room_goto(rm_menu);
        break;
    }
}

if (!debug)
{
    if (mouse_check_button_pressed(mb_left))
    {
        switch (debugClick)
        {
            case 0: 
            if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick++;
            }
            break;
            
            case 1:
            if (mouse_x &gt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick++;
            }
            else if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick = 1;
            }
            break;
            
            case 2:
            if (mouse_x &gt; room_width/2 &amp;&amp; mouse_y &gt; room_height/2)
            {
                debugClick++;
            }
            else if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick = 1;
            }
            break;
            
            case 3:
            if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &gt; room_height/2)
            {
                debugClick++;
            }
            else if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick = 1;
            }
            break;
            
            default:
            if (mouse_x &lt; room_width/2 &amp;&amp; mouse_y &lt; room_height/2)
            {
                debugClick = 1;
            }
            else
            {
                debugClick = 0;
            }
            break;
        }
        if (debugClick == 4)
        {
            show_debug_message("Debug enabled!");
            debug = true;
        }
    }
}
